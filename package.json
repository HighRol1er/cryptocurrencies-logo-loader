{
  "name": "crypto-cex-icons",
  "version": "1.4.0",
  "description": "Cryptocurrency exchange icons for Binance, Upbit, and Bithumb - Optimized WebP images for web applications",
  "main": "index.js",
  "types": "types/crypto-cex-icons.d.ts",
  "sideEffects": false,
  "files": [
    "index.js",
    "types/",
    "icons/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node svg-downloader.js",
    "basic": "node download-svg-basic.js",
    "axios": "node download-svg-axios.js",
    "multiple": "node download-multiple-svg.js",
    "test": "node svg-downloader.js --help",
    "bithumb:ticker": "node fetcher/bithumb-ticker-fetcher.js",
    "bithumb:icon": "node fetcher/bithumb-icon-fetcher.js",
    "bithumb:all": "npm run bithumb:ticker && npm run bithumb:icon",
    "upbit:ticker": "node fetcher/upbit-ticker-fetcher.js",
    "upbit:icon": "node fetcher/upbit-icon-fetcher.js",
    "upbit:all": "npm run upbit:ticker && npm run upbit:icon",
    "binance:ticker": "node fetcher/binance-ticker-fetcher.js",
    "binance:icon": "node fetcher/binance-icon-fetcher.js",
    "binance:all": "npm run binance:ticker && npm run binance:icon",
    "fetch:all": "npm run binance:all && npm run upbit:all && npm run bithumb:all",
    "prepublishOnly": "node -e \"console.log('Publishing crypto-cex-icons package...')\""
  },
  "keywords": [
    "cryptocurrency",
    "crypto",
    "exchange",
    "binance",
    "upbit",
    "bithumb",
    "ticker",
    "icon",
    "image",
    "downloader",
    "resize",
    "webp",
    "sharp",
    "nodejs",
    "api",
    "typescript",
    "react",
    "vue",
    "nextjs"
  ],
  "author": "HighRol1er",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HighRol1er/crypto-cex-img-loader.git"
  },
  "bugs": {
    "url": "https://github.com/HighRol1er/crypto-cex-img-loader/issues"
  },
  "homepage": "https://github.com/HighRol1er/crypto-cex-img-loader#readme",
  "dependencies": {
    "sharp": "^0.33.2"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=14.0.0"
  }
}
